{
  "name": "rerun-workflow",
  "version": "0.0.0",
  "private": true,
  "description": "Re-run Pull Request Workflow",
  "main": "lib/main.js",
  "scripts": {
    "build": "npm run codegen && tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reitermarkus/rerun-workflow.git"
  },
  "keywords": [
    "actions",
    "rerun",
    "workflow"
  ],
  "author": "Markus Reiter <me@reitermark.us>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "ts-is-present": "^1.1.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/typescript": "1.17.10",
    "@graphql-codegen/typescript-document-nodes": "1.17.8",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "@octokit/graphql-schema": "^9.0.0",
    "@octokit/types": "^5.5.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.4.0",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.11.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.0.2",
    "graphql": "^15.3.0",
    "jest": "^26.5.3",
    "jest-circus": "^26.5.3",
    "js-yaml": "^3.14.0",
    "prettier": "2.1.2",
    "ts-jest": "^26.4.1",
    "typescript": "^4.0.3"
  }
}
